                    log({ position: 'tipOpenTimes' });
                } else {
                    log({ position: 'tipClosed' });
                }
            }
        }

        function log(obj) {
            obj.type = "hao123Tip";
            Model.Global.UT.send(obj);
        }
    }, 5000);
