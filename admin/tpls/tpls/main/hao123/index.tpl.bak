    T.cookie.move('STO');

    var FLASH_ID_KEY = "FLASHID",
            BAIDU_ID_KEY = "BAIDUID",
            LAST_ACCESS_KEY = "LAT",
            expires = new Date, lastAccessTime, accessCookie = [];

    lastAccessTime = (new RegExp("(^| )" + LAST_ACCESS_KEY + "=([^;]*)(;|\x24)")).exec(document.cookie);
    if (lastAccessTime) {
        lastAccessTime = lastAccessTime[2];
    }

    accessCookie.push(LAST_ACCESS_KEY, "=", +expires, "; path=/");
    expires.setTime(expires.getTime() + 1000 * 60 * 60 * 24 * 365);
    accessCookie.push("; expires=", expires.toGMTString(), "; domain=hao123.com");
    document.cookie = accessCookie.join('');

    Model.Index.flashStorage.wait(function () {
        var storage, flashId, baiduId;
        storage = Model.Index.flashStorage;
        flashId = storage.getValue(FLASH_ID_KEY);
        if (!flashId) {
            baiduId = (new RegExp("(^| )" + BAIDU_ID_KEY + "=([^;]*)(;|\x24)")).exec(document.cookie);
            if (baiduId) {
                flashId = baiduId[2];
                storage.setValue(FLASH_ID_KEY, flashId);
            }
        }
        if (flashId) {
            document.cookie = FLASH_ID_KEY + "=" + flashId + "; path=/; expires=" + expires.toGMTString() + "; domain=hao123.com";
            Model.Global.UT.send({ type: "flash", embed: antic.embed, ref: antic.ref, pageId: pageId, lastAccess: lastAccessTime || 0 });
